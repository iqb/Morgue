{
    "name": "morgue/morgue",
    "description": "ZipArchive replacement supporting PHP streams.",
    "authors": [
        {
            "name": "Dennis Birkholz",
            "email": "dennis@birkholz.org"
        }
    ],
    "license": "LGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "morgue\\archive\\": "lib/Archive/src/",
            "iqb\\stream\\": "lib/SubStream/src/",
            "iqb\\": "src/"
        },
        "files": [
            "lib/Archive/src/constants.php",
            "lib/SubStream/src/register.php",
            "src/zip/constants.php",
            "src/zip/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "iqb\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-zlib": "*"
    },
    "suggests": {
        "ext-bz2": "Adds support for the BZip2 compression method"
    },
    "require-dev": {
        "ext-zip": "*",
        "ext-bz2": "*",
        "phpunit/phpunit": "^6.4"
    }
}
