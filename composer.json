{
    "name": "iqb/zip",
    "description": "ZipArchive replacement supporting PHP streams.",
    "authors": [
        {
            "name": "Dennis Birkholz",
            "email": "dennis@birkholz.org"
        }
    ],
    "license": "LGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "iqb\\": "src/"
        },
        "files": [
            "src/zip/constants.php",
            "src/zip/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "iqb\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-zlib": "*"
    },
    "require-dev": {
        "ext-zip": "*",
        "phpunit/phpunit": "^6.4"
    }
}
